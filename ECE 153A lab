
// Code to run to produce a counter on an fpga board. Interrupts for button pressing to implement
// stop, start and pause in progress.




/***************************** Include Files *******************************/
#include "sevenSeg.h"
#include <stdio.h>		// Used for printf()
#include "xparameters.h"	// Contains hardware addresses and bit masks
#include "xil_cache.h"		// Cache Drivers
#include "xintc.h"		// Interrupt Drivers
#include "xtmrctr.h"		// Timer Drivers
#include "xtmrctr_l.h" 		// Low-level timer drivers
#include "xil_printf.h" 	// Used for xil_printf()
#include "extra.h" 		// Provides a source of bus contention
#include "xgpio.h" 		// LED driver, used for General purpose I/i
#include "sevenSeg.h" 		// Custom peripheral driver for Seven Segment display
/************************** Function Definitions ***************************/

/* static int x = 0;    //Begin the Grand Loop
 *
 */


int milisec = 0;
int sec = 0;
int sectwo = 0;
int secthree = 0;
int secfour = 0;
XGpio btnPress;
XGpio Gpio;
void milidetector(int miliseconds);
void secondDetector(int seconds);
void allMightyDetector(int miliseconds, int seconds, int sectwo, int secthree, int secfour);
void milisecDetector(int miliseconds);
void secondDetector(int seconds);
void IntrHandler(void *baseaddr_p);
void reset();
int main()
{

	microblaze_register_handler((XInterruptHandler)IntrHandler, (void *)0);
	//extra_method();

	XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
	XTmrCtr_SetLoadReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
	XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, XTC_CSR_LOAD_MASK);
	XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1,	XTC_CSR_ENABLE_TMR_MASK);
	SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);

	while(1){
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111111101000000); ZERO
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111111011111001); ONE
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111110110100100); TWO
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110110000); THREE
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110011001); FOUR
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110010010); FIVE
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110000010); SIX
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111111011111000); SEVEN
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111111100000000); EIGHT
		//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111111100011000); NINE

		//xil_printf("\t%d \n", numClockCycles);
		int numClockCycles = XTmrCtr_GetTimerCounterReg(XPAR_TMRCTR_0_BASEADDR, 1);
		//xil_printf("\t%d \n", numClockCycles);
		//secondDetector(sec);
		//milisecDetector(milisec);
		SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111110001000000);
		allMightyDetector(milisec, sec, sectwo, secthree, secfour);
		//xil_printf("%d\n",numClockCycles);
		if(numClockCycles >= 20000000){


				milisec++;

					XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
					XTmrCtr_SetLoadReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
					XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, XTC_CSR_LOAD_MASK);
					XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1,	XTC_CSR_ENABLE_TMR_MASK);
					if(milisec == 10){
						milisec = 0;
						sec++;
					}
					if(sec == 10){
						sec = 0;
						sectwo++;
					}
					if(sectwo == 10){
						sectwo = 0;
						secthree++;
		}
					if(secthree == 10){
						secthree = 0;
						secfour++;
					}
					if(secfour == 10){
						secfour = 0;

					}
		}
	}
	return 0;
}

void IntrHandler(void *baseaddr_p){

	Xuint32 btnvalue = XGpio_DiscreteRead(&Gpio, 1);
		if(btnvalue == 0x01){
			reset();
		}
}


void reset(){
	milisec = 0;
	sec = 0;
	sectwo = 0;
	secthree = 0;
	secfour = 0;
}



void milisecDetector(int miliseconds){
							if(miliseconds == 0){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111001111000000);
							}
							if(miliseconds == 1){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111001111111001);
							}
							if(miliseconds == 2){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111001110100100);
							}
							if(miliseconds == 3){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110110000);
							}
							if(miliseconds == 4){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110011001);
							}
							if(miliseconds == 5){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110010010);
							}
							if(miliseconds == 6){
								//xil_printf("We in the loop \n");
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110000010);
							}
							if(miliseconds == 7){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111111000);
							}
							if(miliseconds == 8){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110000000);
							}
							if(miliseconds == 9){
								SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110011000);
							}




	}

void secondDetector(int seconds){
							if(seconds == 0){
										SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111001111000000);

													}
													if(seconds == 1){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111001111111001);

													}
													if(seconds == 2){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110100100);

													}
													if(seconds == 3){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110110000);

													}
													if(seconds == 4){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110011001);

													}
													if(seconds == 5){
														//xil_printf("We in the loop \n");
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110010010);

													}
													if(seconds == 6){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110000010);

													}
													if(seconds == 7){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111111000);

													}
													if(seconds == 8){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110000000);

													}
													if(seconds == 9){
														SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110011000);

													}

}






void allMightyDetector(int miliseconds, int seconds, int sectwo, int secthree, int secfour){
						//xil_printf("%d.%d\n", seconds, miliseconds);
						//xil_printf("seconds: %d\n", seconds);


						if(seconds == 0){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);

						}
						if(seconds == 1){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111111001);

						}
						if(seconds == 2){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110100100);

						}
						if(seconds == 3){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110110000);

						}
						if(seconds == 4){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110011001);

						}
						if(seconds == 5){
							//xil_printf("We in the loop \n");
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110010010);

						}
						if(seconds == 6){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110000010);

						}
						if(seconds == 7){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111111000);

						}
						if(seconds == 8){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110000000);

						}
						if(seconds == 9){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110011000);

						}
						if(miliseconds == 0){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111000000);

						}
						if(miliseconds == 1){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111111001);
						}
						if(miliseconds == 2){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110100100);
						}
						if(miliseconds == 3){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110110000);
						}
						if(miliseconds == 4){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110011001);
						}
						if(miliseconds == 5){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110010010);
						}
						if(miliseconds == 6){
							//xil_printf("We in the loop \n");
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110000010);
						}
						if(miliseconds == 7){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111111000);
						}
						if(miliseconds == 8){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110000000);
						}
						if(miliseconds == 9){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110011000);
						}
						if(miliseconds == 0){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111000000);

						}
						if(sectwo == 0){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111111000000);

						}
						if(sectwo == 1){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111111111001);
						}
						if(sectwo == 2){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110100100);
						}
						if(sectwo == 3){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110110000);
						}
						if(sectwo == 4){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110011001);
						}
						if(sectwo == 5){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110010010);
						}
						if(sectwo == 6){
							//xil_printf("We in the loop \n");
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110000010);
						}
						if(sectwo == 7){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111111111000);
						}
						if(sectwo == 8){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110000000);
						}
						if(sectwo == 9){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1110111110011000);
						}
						if(secthree == 0){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111111000000);

						}
						if(secthree == 1){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111111111001);
						}
						if(secthree == 2){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111110100100);
						}
						if(secthree == 3){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111110110000);
						}
						if(secthree == 4){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111110011001);
						}
						if(secthree == 5){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111110010010);
						}
						if(secthree == 6){
							//xil_printf("We in the loop \n");
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111110000010);
						}
						if(secthree == 7){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111111111000);
						}
						if(secthree == 8){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111110000000);
						}
						if(secthree == 9){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1101111110011000);
						}
						if(secfour == 0){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111111000000);

						}
						if(secfour == 1){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111111111001);
						}
						if(secfour == 2){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111110100100);
						}
						if(secfour == 3){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111110110000);
						}
						if(secfour == 4){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111110011001);
						}
						if(secfour == 5){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111110010010);
						}
						if(secfour == 6){
							//xil_printf("We in the loop \n");
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111110000010);
						}
						if(secfour == 7){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111111111000);
						}
						if(secfour == 8){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111110000000);
						}
						if(secfour == 9){
							SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1011111110011000);
						}
						//XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
						//XTmrCtr_SetLoadReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
						//XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, XTC_CSR_LOAD_MASK);
						//XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1,	XTC_CSR_ENABLE_TMR_MASK);



}




/*
void milidetector(int miliseconds){
	if(miliseconds == 0){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111000000);
					}
					if(miliseconds == 1){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111111001);
					}
					if(miliseconds == 2){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110100100);
					}
					if(miliseconds == 3){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110110000);
					}
					if(miliseconds == 4){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110011001);
					}
					if(miliseconds == 5){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110010010);
					}
					if(miliseconds == 6){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110000010);
					}
					if(miliseconds == 7){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101111111000);
					}
					if(miliseconds == 8){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110000000);
					}
					if(miliseconds == 9){
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
						//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
						SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111101110011000);
					}
					XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
					XTmrCtr_SetLoadReg(XPAR_TMRCTR_0_BASEADDR, 1, 0);
					XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1, XTC_CSR_LOAD_MASK);
					XTmrCtr_SetControlStatusReg(XPAR_TMRCTR_0_BASEADDR, 1,	XTC_CSR_ENABLE_TMR_MASK);

}
void secondDetector(int seconds){




	//SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
if(seconds == 0){
	SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111000000);
	SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
}
if(seconds == 1){
	SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011111111001);
	SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
}
if(seconds == 2){
	SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1111011110100100);
	SEVENSEG_mWriteReg(XPAR_SEVENSEG_0_S00_AXI_BASEADDR, 0, 0b1000000001000000);
}
if(seconds == 3){
